{
    "manifestVersion": 1,
    "id": "pr-workitems",
    "version": "1.0.6",
    "name": "PR workitems",
    "description": "A pull request menu extension to create a new workitem and associate it with current pull request",
    "publisher": "mohitbagra",
    "icons": {
        "default": "images/logo.png"
    },
    "scopes": [
    	"vso.work",
        "vso.work_write"
	 ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "tags": [
        "Work Item",
        "Pull Requests"
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mohitbagra/vsts-extensions"
    },    
    "branding": {
        "color": "#106ebe",
        "theme": "dark"
    },
    "categories": [
        "Code"
    ],
    "contributions": [
        {
            "id": "context-menu",
            "type": "ms.vss-web.action",
            "targets": [
                "ms.vss-code-web.pull-request-action-menu"
            ],
            "properties": {
                "group": "contributed",
                "uri": "html/index.html",
                "icon": "images/logo.png",
                "text": "Link to a new workitem"
            }
        }
    ]
}
